import java.util.ArrayList;



public class PoundKilogramConverter_5_5 {

	public static void main(String[] args) {
		
		Double kilo = 1.0, pound = 2.2;
		String print, kilograms = "Kilograms", pounds = "Pounds";
		
		System.out.printf("%-13s%13s", kilograms, pounds);
		for(int i = 1; i < 200; i+=2){
			print = String.format("%-13d%13.2f", i, (i * 2.2));
		}

	}

}
