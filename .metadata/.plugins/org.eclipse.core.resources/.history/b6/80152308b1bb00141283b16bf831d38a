import java.util.Scanner;

/**
 * @author Armando Dollia
 * dollia.armando@gmail.com
 *
 */

public class FutureDates_3_5 {

	public static void main(String[] args) {
		Integer checksum = 0, lastDigit, counter = 1;
		String firstDigits, test = "";
		int[] array = new int[9];
		Scanner input = new Scanner(System.in);
		
		System.out.println("Enter the first 9 digits of an ISBN or integer: ");
		firstDigits = input.next();
		
		for(int i = 0; i < 9; i++){
		array[i] = Integer.getInteger(firstDigits.substring(i, i++));
		}
		
		for(int i = 0; i < 9; i++){
			checksum += (array[i] * i++);
			counter++;
		}
		
		for(int i = 0; i < 9; i++){
			test += (array[i]);
			counter++;
		}
		
		System.out.println(test);

		input.close();
	}

}
